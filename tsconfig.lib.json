{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/out-tsc",
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "types": [],
    "skipLibCheck": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "lib": [
      "es5",
      "es2015.promise",
      "es2015.collection",
      "es2015.iterable",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableResourceInlining": true
  },
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    "node_modules/@types/node/**/*"
  ]
}